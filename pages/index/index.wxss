/**index.wxss**/
.nav {
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 84rpx;
  line-height: 84rpx;
  background-color: #001529;
  color: #faebd7;
}

.nav__logo {
  margin-left: 30rpx;
  font: bold 32rpx sans-serif;
  color: #faebd7;
}

.nav__add {
  height: 32rpx;
  width: 32rpx;
  margin-right: 30rpx;
  background-image: url("data:image/png;base64,");
  background-size: 100% 100%;
}

.wrap {
  width: 80%;
  margin: 35rpx auto;
  color: #515151;
}

.collapse-panel {
  display: flex;
  align-items: center;
  height: 58rpx;
  font-size: 24rpx;
  padding: 0 20rpx;
  background-color: #faebd7;
  border: 1px solid #d9d9d9;
  border-top-left-radius:4px;
  border-top-right-radius:4px;
  border-bottom:0;
  cursor: pointer;
}

.center {
  display: flex;
  align-items: center;
  justify-content: center;
}

.spread {
  width: 28rpx;
  height: 28rpx;
  margin-right: 10rpx;
  background-image: url("data:image/png;base64,");
  background-size: 100% 100%;
}

.collapse {
  width: 28rpx;
  height: 28rpx;
  background-image: url("data:image/png;base64,");
  background-size: 100% 100%;
}

.todo-icon {
  width: 28rpx;
  height: 28rpx;
  margin-left: 65%;
  margin-right: 10rpx;
  background-image: url("data:image/png;base64,");
  background-size: 100% 100%;
}

.empty{
  flex-direction: column;
  padding:80rpx 0;
  font-size: 24rpx;
  color:rgba(0, 0, 0, 0.25);
    border: 1px solid #d9d9d9;
}

.box {
  width: 64rpx;
  height: 64rpx;
  background-image: url("data:image/png;base64,");
  background-size: 100% 100%;
}

.spread-rotate{
  transform:rotate(90deg);
}

.collapse-rotate{
  transform:rotate(-90deg);
}

.picker {
  display: inline-flex;
  margin: 5rpx;
  padding: 4rpx;
  /* line-height: 1.5715; */
  color: rgba(0, 0, 0, 0.65);
  font-size: 30rpx;
  border: 1px solid #d9d9d9;
  border-radius: 2px;
  background-color: #fff;
}
